page{
  height:100%;
  overflow-y: initial !important;
  color: #333;
}
.mask{
  position: absolute;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, .5);
  z-index: 99;  
}
.logotitle{
  width: 265rpx;
  height: 100rpx;
  display: block;
  margin: auto;
  padding: 70rpx 0 40rpx;
}
.Member{
  border-radius:30rpx;
  width:530rpx;
  height:300rpx;
  display:block;
  margin:60rpx auto;
}

.textbox{
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
  padding: 40rpx 0;
}
.textbox view{
  padding: 5rpx;
}
.bottomBtn{
  width: 100%;
  background: linear-gradient(to right, #FD9E67 , #FF7468);
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: fixed;
  bottom: 0;
  color: #fff;
}
.bottomBtn .t1{
  font-size: 30rpx;
  margin-right: 20rpx;
}
.bottomBtn .t2{
  font-size: 30rpx;
}



.Popup{
  position: fixed;
  bottom: -751px;
  background: #FBFBFB;
  width: 100%;
  z-index: 100;
  transition: all .2s;
  overflow: hidden;
}
.Popup .title{
  position: absolute;
  font-size: 34rpx;
  width: 400rpx;
  padding: 20rpx 0;
}

.Popup .t1{
  font-weight: 600;
  font-size: 36rpx;
  position: absolute;
  top: 200rpx;
}
.Popup .t2{
  font-size: 24rpx;
  position: absolute;
  right: 40rpx;
  top: 200rpx;
  font-weight: 600;
}
.Popup .t3{
  color: #2788E0;
  border: 1px solid #2788E0;
  padding: 4rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.Popup .t4{
  color: #929292;
  padding: 4rpx;
  border-radius: 4rpx;
  font-size: 24rpx;
  margin-left: 10rpx;
  display: inline-block;
}
.Popup .t5{
  color: #929292;
  padding: 4rpx;
  border-radius: 4rpx;
  font-size: 24rpx;
  margin-left: 10rpx;
  display: inline-block;
}
.Popup .Selected{
  position: absolute;
  z-index: 999;
  top:150rpx;
  left:275rpx;
}
.Popup .close{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 40rpx;
  height: 40rpx;
}

.active{
  background: #2788E0;
  border: none !important;
}
.suerBtn{
  background: linear-gradient(to right, #FD9E67 , #FF7468);
  border-radius: 40rpx;
  height: 80rpx;
  margin: 30rpx auto;
  line-height: 80rpx; 
  font-size: 35rpx;
  position: relative;
  width: 94%;
  overflow: hidden;
}
.suerBtn image{
  position: absolute;
  width: 100%;
  height: 80rpx;
  z-index: -1;

}
.suerBtn text{
  display: block;
  text-align: center;
  color: #fff;
}


.location{
  display: flex;
  justify-content: space-between;
  padding: 30rpx 20rpx;
  border-bottom: 20rpx solid #EBEBEB;
  background: #fff;
  position: relative;
}
.location .shlocation text{
  font-size: 36rpx;
}
.location .xxlocation{
  font-size: 26rpx;
  color: #8e8e8e;
  padding: 10rpx 0;
}
.location .change{
  position: absolute;
  right: 80rpx;
  top: 60rpx;
  font-size: 26rpx;
  color: #8e8e8e;
}
.location image{
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}
.location text{
  font-size: 26rpx;
}
.location .right-icon{
  width: 25rpx;
  height: 25rpx;
  vertical-align: middle;
  display: inline-block;
  margin-top: 25rpx;
}
.location .locationbox1{
  display: flex;
  justify-content: space-between;
  width: 100%;
}


.Regulations{
  display: flex;
  flex-direction: row;
  padding: 20rpx 20rpx;
}
.Regulations .gx{
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;

}
.Regulations .Regulationst1{
  font-size: 30rpx;
  color: #8e8e8e;
}
.Regulations .Regulationst2{
  font-size: 30rpx;
}


.scroll-class{
  width:100%; 
}

.product-box{
  white-space: nowrap;
}
.product-box .product-box-01{
  display: inline-block;
  width: 280rpx;
  padding: 0 10rpx;
  position: relative;
}
.product-box .product-box-01 .product-img{
  width: 100%;
}
.product-box .product-box-01 .gou{
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  z-index: 99;
  left: 50%;
  top: 50%;
  margin-left: -100rpx;
  margin-top: -170rpx;
  opacity: 0;
}
.product-box .product-box-01 .product-name{
  font-size: 30rpx;
  color: #333;
  white-space: normal;
}
.product-box .product-box-01 .product-p-n-box{
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.product-box .product-box-01 .product-p-n-box .product-price{
  color: #FF7468;
  font-size: 26rpx;
  font-weight: 600;
}
.product-box .product-box-01 .product-p-n-box .product-num{
  font-size: 26rpx;
  font-weight: 600;
  margin-right: 10rpx;
}

.caiseborder{
  width: 10000px;
  height: 15rpx;
  background: linear-gradient(45deg, #FF7468 25%, #FD9E67 0, #FD9E67 50%, #FF7468 0, #FF7468 75%, #FD9E67 0); 
  background-size: 20px 20px;
  position: absolute;
  bottom: 0;
}

.opacity07{
  opacity: .7 !important;
  transition: all .5s;
}